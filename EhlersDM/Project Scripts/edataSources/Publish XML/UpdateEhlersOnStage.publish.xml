<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>Ehlers</TargetDatabaseName>
    <DeployScriptFileName>Ehlers.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=EHLERS-SS4;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <CreateNewDatabase>False</CreateNewDatabase>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <DeployDatabaseInSingleUserMode>True</DeployDatabaseInSingleUserMode>
    <BackupDatabaseBeforeChanges>True</BackupDatabaseBeforeChanges>
    <DropObjectsNotInSource>True</DropObjectsNotInSource>
    <DropPermissionsNotInSource>True</DropPermissionsNotInSource>
    <DropRoleMembersNotInSource>True</DropRoleMembersNotInSource>
    <IgnoreAnsiNulls>False</IgnoreAnsiNulls>
    <IgnoreFillFactor>False</IgnoreFillFactor>
    <IgnoreFullTextCatalogFilePath>False</IgnoreFullTextCatalogFilePath>
    <IgnoreIndexPadding>False</IgnoreIndexPadding>
    <IgnoreKeywordCasing>False</IgnoreKeywordCasing>
    <IgnoreQuotedIdentifiers>False</IgnoreQuotedIdentifiers>
    <IgnoreSemicolonBetweenStatements>False</IgnoreSemicolonBetweenStatements>
    <IgnoreWhitespace>False</IgnoreWhitespace>
    <PopulateFilesOnFileGroups>False</PopulateFilesOnFileGroups>
    <ScriptDeployStateChecks>True</ScriptDeployStateChecks>
    <ScriptDatabaseCompatibility>False</ScriptDatabaseCompatibility>
    <TreatVerificationErrorsAsWarnings>True</TreatVerificationErrorsAsWarnings>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="AgentJobOwner">
      <Value>EHLERS\Admin</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BuildSQLAgentJobs">
      <Value>YES</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="edata">
      <Value>edata</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="EnableRI">
      <Value>YES</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="EnableTriggers">
      <Value>YES</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="PostDeployDataChanges">
      <Value>NO</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="PreDeployDataChanges">
      <Value>NO</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="ProductionServer">
      <Value>EHLERS-SS3</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="TargetServer">
      <Value>EHLERS-SS4</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>